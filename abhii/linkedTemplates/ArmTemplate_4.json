						"          Salary,",
						"          LastUpdatedDate,",
						"          status,",
						"          hashkey = hashkey1",
						"     )) ~> sink1",
						"alterRow1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          EmpID as integer,",
						"          EmpName as string,",
						"          Department as string,",
						"          Salary as integer,",
						"          LastUpdatedDate as date,",
						"          status as integer,",
						"          hashkey as string",
						"     ),",
						"     deletable:false,",
						"     insertable:false,",
						"     updateable:true,",
						"     upsertable:false,",
						"     keys:['EmpID'],",
